<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <base href="{{ url.base }}">
    <script>
        window._global = {
            id: '{{ id }}',
            url: {
                socket: '{{ url.websocket }}',
                monaco: '{{ url.monaco }}/vs',
                base: '{{ url.base }}',
                prefix: '{{ url.prefix }}'
            },
            userName: '{{ userName }}'
        };
    </script>
    {% block style %} {% endblock %}
</head>
<body>
    {% block content %} {% endblock %}

    <script src="{{ resource.base }}{{ resource.js.vendor }}"></script>
    {% block script %} {% endblock %}
</body>
</html>