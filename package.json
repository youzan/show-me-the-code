{
  "name": "show-me-the-code",
  "version": "0.0.0",
  "scripts": {
    "serve": "better-npm-run serve",
    "dev": "better-npm-run dev",
    "prod": "better-npm-run prod"
  },
  "betterScripts": {
    "dev": {
      "env": {
        "NODE_ENV": "development"
      },
      "command": "node --max_old_space_size=4096 ./node_modules/.bin/webpack -w"
    },
    "serve": {
      "env": {
        "NODE_ENV": "development"
      },
      "command": "node --max_old_space_size=4096 ./node_modules/.bin/webpack-dev-server --hot"
    },
    "prod": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "webpack --config webpack.executors.js && webpack --config webpack.app.js"
    }
  },
  "private": true,
